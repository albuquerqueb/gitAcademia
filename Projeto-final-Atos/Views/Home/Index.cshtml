@{
    ViewData["Title"] = "Pagina Inicial";
}

<div id="carouselExampleFade" class="container carousel slide carousel-fade" data-bs-ride="carousel">
    
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="~/css/imagem-1.jpg" class="d-block w-100" alt="50">
    </div>
    <div class="carousel-item">
      <img src="~/css/imagem-3.jpg" class="d-block w-100" alt="50">
    </div>
    <div class="carousel-item">
      <img src="~/css/imagem-2.jpg" class="d-block w-100" alt="50">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>

<div class="text-center">
    <h1 class="display-4">Meu Destino</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
